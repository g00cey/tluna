{
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "jedi": {
      "enable": true,
      "startupMessage": false,
      "executable.command": "~/.anyenv/envs/pyenv/shims/jedi-language-server"
  },
  {
    "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        }
    }
}
}
